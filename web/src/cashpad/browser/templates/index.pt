<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <meta http-equiv="X-UA-Compatible" content="IE=7" />
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <title>www.ipadkassasysteem.nl</title>
</head>
<body>
  <div id="hld">
    <div class="wrapper">    <!-- wrapper begins -->
      <div id="header">
        <div class="hdrl"></div>
        <div class="hdrr"></div>
        <h1><a href="#">iPad kassa</a></h1>
        <ul id="nav">
          <li>
            <a tal:attributes="href view/application_url">Users</a>
            <ul>
              <li tal:repeat="user context/user/values">
                <a tal:attributes="href python:view.url(user)"
                   tal:content="user/name" />
              </li>
            </ul>
          </li>
        </ul>
        <p class="user"><a tal:attributes="href python:view.url('logout')">Logout</a></p>
      </div>    <!-- #header ends -->

      <div class="block">
        <div class="block_head">
          <div class="bheadl"></div>
          <div class="bheadr"></div>
          <h2>Users</h2>
        </div>    <!-- .block_head ends -->

        <div class="block_content">
          <h1>Choose a user</h1>
          <ul>
            <li tal:repeat="usert context/user/values">
              <a tal:attributes="href python:view.url(usert)"
                 tal:content="usert" />
            </li>
          </ul>
        </div>    <!-- .block_content ends -->

      </div>    <!-- .block ends -->
      <div id="footer">
        <p class="left"><a tal:attributes="href view/application_url">www.ipadkassasysteem.nl</a></p>
        <p class="right">powered by <a href="http://themeforest.net/item/adminus-beautiful-admin-panel-interface/94668?ref=enstyled">Adminus</a> v1.4</p>
      </div>
    </div>
  </div>
</body>
</html>
